{
	"name": "ClipPage",
	"description": "",
	"version": "0.0.0",
	"author": "sekai013",
	"permissions": [
		"storage",
		"tabs"
	],
	"browser_action": {
		"default_popup": "background/popup.html",
		"default_icon": {
			"19": "img/icon19.png",
			"38": "img/icon38.png"
		},
		"default_title": "ClipPage"
	},
	"content_scripts": [{
		"matches": ["http://*/*", "file://*"],
		"js": ["content/content.js"]
	}],
	"background": {
		"scripts": [
			"background/js/lib/jquery-2.1.1.min.js",
			"background/js/lib/underscore.js",
			"background/js/lib/backbone.js",
			"background/js/lib/backbone.localStorage.js",
			"background/js/bgModels.js",
			"background/js/background.js"
		],
		"persistent": false
	},
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"manifest_version": 2
}

